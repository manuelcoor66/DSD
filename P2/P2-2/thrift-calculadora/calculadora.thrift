service Calculadora{
   void ping(),
   i32 suma(1:i32 num1, 2:i32 num2),
   i32 resta(1:i32 num1, 2:i32 num2),
   i32 multiplicacion(1:i32 num1, 2:i32 num2),
   double division(1:double num1, 2:double num2),
   i32 modulo(1:i32 num1, 2:i32 num2),
   i32 elevar(1:i32 num1, 2:i32 num2),
   double raiz_cuadrada(1:i32 num1),
   double logaritmo(1:i32 num1),
   i32 valor_absoluto(1:i32 num1),
}
